{
  "homepage": "https://walgab.github.io/Portfolio_like_project",
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "history": "^5.3.0",
    "live-server": "^1.2.2",
    "parcel": "^2.8.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0"
  },
  "scripts": {
    "dev": "parcel src/index.html",
    "build": "parcel build src/index.html --public-url \"https://walgab.github.io/Portfolio_like_project/\"",
    "build-local": "parcel build src/index.html --dist-dir dist-local",
    "start": "npm run build-local && live-server dist-local",
    "predeploy": "npm run build",
    "deploy": "npm run build && gh-pages -d dist"
  },
  "devDependencies": {
    "eslint": "^8.33.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-react": "^7.32.2",
    "gh-pages": "^5.0.0",
    "process": "^0.11.10"
  }
}
